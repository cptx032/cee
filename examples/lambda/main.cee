@import {
    stdio, stdbool
}

@fn filter_number(@delegate number_operation {bool, int}, int number) bool {
    return number_operation(number);
}

@fn main() int {
    int number = 3;
    bool result = filter_number(@fn (int number) bool {
        return (number % 2) == 0;
    }, number);
    printf("number = %d, result = %d\n", number, result);
    return 0;
}
